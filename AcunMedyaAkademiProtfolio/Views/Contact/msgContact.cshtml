@model AcunMedyaAkademiProtfolio.Models.Tbl_Contact
@{
    ViewBag.Title = "msgContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h2>Mesaj Detayları</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <td>@Model.ContactId</td>
    </tr>
    <tr>
        <th>Ad Soyad</th>
        <td>@Model.ContactNameSurname</td>
    </tr>
    <tr>
        <th>Mail</th>
        <td>@Model.ContactEmail</td>
    </tr>
    <tr>
        <th>Telefon</th>
        <td>@Model.ContactPhone</td>
    </tr>
    <tr>
        <th>Mesaj</th>
        <td>@Model.ContactMessage</td>
    </tr>
    <tr>
        <th>Gönderim Tarihi</th>
        <td>@Model.SendDate</td>
    </tr>
</table>

<a href="/Contact/ContactList" class="btn btn-primary">Geri Dön</a>

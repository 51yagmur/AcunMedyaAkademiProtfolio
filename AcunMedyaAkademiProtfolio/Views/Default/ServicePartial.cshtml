@model List<AcunMedyaAkademiProtfolio.Models.Tbl_Service>

<div id="services" class="section lb">
    <div class="container">
        <div class="section-title text-left">
            <h3>Hizmetlerim</h3>
            <p>talep edilen projelerde hızlı ve düzenli tasarımlar yapılır</p>
        </div><!-- end title -->

        <div class="row">
        @foreach(var item in Model) {
            <div class="col-md-4">
                <div class="services-inner-box">
                    <div class="ser-icon">
                        <div class="icon d-flex align-items-center justify-content-center"><span><img src="@item.ServiceİmageUrl " style="width:80px;height:80px;" /></span></div>
                    </div>
                    <h2>@item.ServiceTitle</h2>
                    <p>@item.ServiceDescription</p>
                </div>
            </div> }<!-- end col -->
           
        </div><!-- end row -->
    </div><!-- end container -->
</div>